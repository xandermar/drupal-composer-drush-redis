{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.7",
        "drupal/accordion_menus": "^3.0",
        "drupal/address": "^1.7",
        "drupal/admin_toolbar": "^2.0",
        "drupal/adsense": "^1.1",
        "drupal/advagg": "^4.0",
        "drupal/antibot": "^1.2",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/backup_migrate": "^4.0",
        "drupal/block_class": "^1.1",
        "drupal/block_content_permissions": "^1.7",
        "drupal/bootstrap": "^3.21",
        "drupal/bootstrap_library": "^1.11",
        "drupal/bulk_user_registration": "^1.1@alpha",
        "drupal/chosen": "^2.8",
        "drupal/ckeditor_bootstrap_grid": "^1.2",
        "drupal/ckeditor_media_embed": "^1.7",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/content_access": "^1.0@alpha",
        "drupal/content_sync": "^2.1",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/custom_search": "^1.0@beta",
        "drupal/ds": "^3.5",
        "drupal/easy_breadcrumb": "^1.12",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/email_registration": "^1.0@RC",
        "drupal/entity_browser": "^2.2",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/facets": "^1.4",
        "drupal/feeds": "^3.0@alpha",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_formatter_class": "^1.1",
        "drupal/field_permissions": "^1.0@RC",
        "drupal/file_entity": "^2.0@beta",
        "drupal/filebrowser": "^2.0",
        "drupal/flexslider": "^2.0@beta",
        "drupal/font_awesome": "^1.2",
        "drupal/fontawesome": "^2.14",
        "drupal/fontawesome_menu_icons": "^1.5",
        "drupal/force_password_change": "^1.3",
        "drupal/front": "^1.0@beta",
        "drupal/gdoc_field": "^1.1",
        "drupal/google_analytics": "^3.0",
        "drupal/honeypot": "^1.29",
        "drupal/imce": "^1.7",
        "drupal/libraries": "^3.0@alpha",
        "drupal/login_redirect_per_role": "^1.4",
        "drupal/mass_password_change": "^1.0@RC",
        "drupal/mass_pwreset": "^1.0@alpha",
        "drupal/media_entity": "^1.8",
        "drupal/menu_block": "^1.5",
        "drupal/menu_block_title": "^1.1",
        "drupal/metatag": "^1.10",
        "drupal/module_filter": "^3.1",
        "drupal/multi_node_add": "^1.0@alpha",
        "drupal/nice_menus": "^1.0@beta",
        "drupal/nodeaccess": "^1.0@beta",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panels": "^4.4",
        "drupal/paragraphs": "^1.10",
        "drupal/pathauto": "^1.6",
        "drupal/phpmailer": "^3.0@beta",
        "drupal/poll": "^1.3",
        "drupal/printfriendly": "^3.4",
        "drupal/protected_pages": "^1.1",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/radix_layouts": "^4.1",
        "drupal/realname": "^1.0@RC",
        "drupal/recaptcha": "^2.4",
        "drupal/redirect": "^1.4",
        "drupal/redirect_after_login": "^2.5",
        "drupal/redis": "^1.2",
        "drupal/rules": "^3.0@alpha",
        "drupal/search_api": "^1.15",
        "drupal/search_api_page": "^1.0@beta",
        "drupal/search_api_solr": "^3.7",
        "drupal/shs": "^1.0@alpha",
        "drupal/simple_access": "^3.0@alpha",
        "drupal/smtp": "^1.0@beta",
        "drupal/starrating": "^4.0@alpha",
        "drupal/superfish": "^1.3",
        "drupal/svg_image": "^1.9",
        "drupal/swiftmailer": "^1.0@beta",
        "drupal/tamper": "^1.0@alpha",
        "drupal/taxonomy_machine_name": "^1.0@beta",
        "drupal/taxonomy_manager": "^1.0@alpha",
        "drupal/text_summary_options": "^1.0",
        "drupal/views_bootstrap": "^4.1",
        "drupal/views_bulk_operations": "^3.3",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/views_flipped_table": "^1.3",
        "drupal/views_xml_backend": "^1.0@alpha",
        "drupal/we_megamenu": "^1.7",
        "drupal/webform": "^5.6",
        "drupal/webform_views": "^5.0@alpha",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush/drush": "^10.1",
        "predis/predis": "^1.1",
        "wikimedia/composer-merge-plugin": "^1.4"
    },
    "require-dev": {
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-core-project-message": {
            "include-keys": ["homepage", "support"],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",

                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
